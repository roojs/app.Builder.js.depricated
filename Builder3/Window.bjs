                                                            "|init": "function() {\n    XObject.prototype.init.call(this);\n   this.el.set_policy (Gtk.PolicyType.AUTOMATIC, Gtk.PolicyType.AUTOMATIC);\n}\n",
                                                            "|shadow_type": "Gtk.ShadowType.IN",
                                                            "|xns": "Gtk",
